#!/bin/bash

script_dir="$( cd "$( dirname "${BASH_SOURCE[0]}" )" && pwd )"

#---------- input --------------
chr=6
beginning=rs1001015
ending=rs3734693

#---------- script location --------------
run_all_fam_script=$script_dir/091_run_all_fam

#---------- run --------------
$run_all_fam_script $chr $beginning $ending



