#!/bin/bash

script_dir="$( cd "$( dirname "${BASH_SOURCE[0]}" )" && pwd )"

data_dir=$script_dir/../data

out_dir=$script_dir/../out
out_file=$out_dir/further_analysis.xls

#---------- script location --------------
analyze_script=$script_dir/010_analyze.py

#---------- run --------------
python $analyze_script $data_dir $out_file

